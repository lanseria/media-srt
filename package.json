{
  "name": "media-srt",
  "version": "16.0.10",
  "packageManager": "pnpm@7.5.0",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "electron:generate-icons": "electron-icon-builder --input=./logo.png --output=build --flatten",
    "dev": "npm run dev:all",
    "dev:all": "concurrently -n=vite,main -c=#646cff,#74b1be \"npm run dev:vite\" \"npm run dev:main\"",
    "dev:vite": "vite",
    "dev:main": "rimraf dist/main && node -r ts-node/register script/build.main --env=development --watch",
    "build:vite": "vite build",
    "build:main": "rimraf dist/main && node -r ts-node/register script/build.main --env=production",
    "build": "npm run build:vite && npm run build:main && electron-builder --config electron-builder.config.js",
    "build:github": "npm run build:vite && npm run build:main && electron-builder --config electron-builder.github.config.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^7.7.1",
    "@vueuse/rxjs": "^7.7.1",
    "bytenode": "^1.3.6",
    "cos-js-sdk-v5": "^1.3.9",
    "cos-nodejs-sdk-v5": "^2.11.12",
    "dayjs": "^1.11.4",
    "dexie": "^3.2.2",
    "express": "^4.18.1",
    "indexeddb-export-import": "^2.1.5",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nanoid": "^3.3.4",
    "pinia": "^2.0.17",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "store": "^2.0.12",
    "tencentcloud-sdk-nodejs": "^4.0.389",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "wavesurfer.js": "^6.2.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/minimist": "^1.2.2",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^18.6.3",
    "@types/wait-on": "^5.3.1",
    "@types/wavesurfer.js": "^6.0.3",
    "@types/which": "^2.0.1",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.37",
    "chalk": "^4.1.2",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "electron": "^16.2.8",
    "electron-builder": "^22.14.13",
    "electron-icon-builder": "^2.0.1",
    "esbuild": "^0.14.51",
    "minimist": "^1.2.6",
    "naive-ui": "^2.32.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^0.39.2    ",
    "wait-on": "^6.0.1"
  }
}
